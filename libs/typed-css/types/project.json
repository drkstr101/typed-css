{
  "root": "libs/typed-css/types",
  "sourceRoot": "libs/typed-css/types",
  "projectType": "library",
  "targets": {
    "install": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "mkdir -p dist/@typed-css/types",
          "cp libs/typed-css/types/index.d.ts dist/@typed-css/types/index.d.ts",
          "cp libs/typed-css/types/package.json dist/@typed-css/types/package.json",
          "rm -rf node_modules/@types/typed-css",
          "cp -r dist/@typed-css/types node_modules/@types/typed-css"
        ],
        "parallel": false,
        "outputPath": [
          "dist/@typed-css/types",
          "node_modules/@types/typed-css"
        ]
      }
    }
  }
}
